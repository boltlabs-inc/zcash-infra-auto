{
  "network": "mainnet",
  "port": 3001,
  "services": [
    "bitcoind",
    "web"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "/home/zcash/.zcash",
        "exec": "/usr/bin/zcashd"
      }
    },
    "insight-api-zcash": {
      "routePrefix": "api"
    },
    "insight-ui-zcash": {
      "apiPrefix": "api",
      "routePrefix": ""
    }
  }
}
